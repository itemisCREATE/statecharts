                  name="semanticHint"
                  value="Region">
            </param>
         </metamodelType>
         <metamodelType
               eclass="State"
               edithelper="org.yakindu.sct.ui.editor.edithelper.StateEditHelper"
               icon="icons/obj16/State-16.png"
               id="org.yakindu.sct.ui.editor.State"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               name="State">
            <param
                  name="semanticHint"
