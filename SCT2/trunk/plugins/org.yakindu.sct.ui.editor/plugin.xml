               name="Transition">
            <param
                  name="semanticHint"
                  value="Transition">
            </param>
         </metamodelType>
         <metamodelType
               eclass="Choice"
               edithelper="org.yakindu.sct.ui.editor.edithelper.VertexEditHelper"
               icon="icons/obj16/Choice-16.png"
