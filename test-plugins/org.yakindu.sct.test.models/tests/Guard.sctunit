testclass Guard for statechart Guard {
	@Test
	operation guardTest(){
		enter
		assert active (Guard.main_region.A)
		raise Event1
		cycle
		
		assert active (Guard.main_region.A)
		raise Event2 
		cycle
		
		assert active (Guard.main_region.B)
		raise Return
		cycle
		
		assert active (Guard.main_region.A)
		raise Event1 
		cycle
		
		assert active (Guard.main_region.B)
		raise Return
		cycle
		 
		assert active (Guard.main_region.A)  
	}
}  